{
  "name": "smileyhouse-server",
  "version": "1.0.0",
  "description": "A port of the game Smiley House in node.",
  "main": "main.js",
  "scripts": {
    "start": "node --harmony main.js",
    "dev": "NODE_ENV=development DEBUG=sh:* node-dev --harmony main.js",
    "debug": "NODE_ENV=development DEBUG=* node debug --harmony main.js",
    "test": "NODE_ENV=test DEBUG=sh:* mocha --recursive test",
    "proxy": "node --harmony tools/proxy.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/bschlenk/smileyhouse-server.git"
  },
  "keywords": [
    "smileyhouse"
  ],
  "author": "Brian Schlenker <bschlenk@umich.edu>",
  "license": "GPL-3.0",
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "colors": "^1.1.2",
    "config": "^1.19.0",
    "debug": "^2.2.0",
    "email-validator": "^1.0.4",
    "lokijs": "^1.3.16"
  },
  "devDependencies": {
    "eslint": "^2.6.0",
    "eslint-config-airbnb": "^6.2.0",
    "mocha": "^2.4.5"
    "proxyquire": "^1.7.10",
    "sinon": "^1.17.6"
  }
}
